BX.namespace("BX.Bitrix24.NotifyPanel");BX.Bitrix24.NotifyPanel={init:function(params){if(typeof params=="object"){this.panelMessage=params.panelMessage||"";this.panelColor=params.panelColor||"red";this.closeAfter15Seconds=params.closeAfter15Seconds==="Y";if(params.closeCallback){BX.addCustomEvent("BX.Bitrix24.NotifyPanel:onClosePanel",eval(params.closeCallback))}}},show:function(){if(!BX.type.isNotEmptyString(this.panelMessage))return;var e=BX.create("div",{attrs:{className:"bx24-tariff-notify bx24-tariff-notify-show bx24-tariff-notify-panel","data-role":"b24-tariff-notify"},children:[BX.create("div",{props:{className:"bx24-tariff-notify-wrap "+(this.panelColor==="blue"?"bx24-tariff-notify-blue":"bx24-tariff-notify-red")},children:[BX.create("span",{props:{className:"bx24-tariff-notify-text"},html:this.panelMessage}),BX.create("span",{props:{className:"bx24-tariff-notify-text-reload"},children:[BX.create("span",{props:{className:"bx24-tariff-notify-text-reload-title"},html:"x"})],events:{click:function(){BX.removeClass(e,"bx24-tariff-notify-show");BX.onCustomEvent("BX.Bitrix24.NotifyPanel:onClosePanel",[])}}})]})]});if(BX.type.isDomNode(BX("creatorconfirmed"))){BX("creatorconfirmed").style.display="none"}var t=document.body.querySelector(".bx-layout-table");if(t){t.parentNode.insertBefore(e,t);if(this.closeAfter15Seconds){setTimeout(function(){BX.removeClass(e,"bx24-tariff-notify-show");if(BX.type.isDomNode(BX("creatorconfirmed"))){BX("creatorconfirmed").style.display="block"}},15e3)}}},onSalePanelClose_12_2018:function(){BX.userOptions.save("bitrix24","notify_panel_sale_12_2018","closed","Y")},onSalePanelClose28:function(){BX.userOptions.save("bitrix24","notify_panel_sale_28","closed","Y")},onTelephonyBalancePanelClose:function(){BX.userOptions.save("bitrix24","notify_panel_telephony_balance","close_timestamp",Math.floor((new Date).getTime()/1e3))},onTelephonyFreePlanWarningClose:function(){BX.ajax.runAction("voximplant.notification.setNotificationSeen")},onRestPanelClose:function(){BX.userOptions.save("bitrix24","notify_panel_rest_access","close_timestamp",Math.floor((new Date).getTime()/1e3))}};
